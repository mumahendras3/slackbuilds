# This is a service to mount procfs
foreground { echo "Starting oneshot service:  mount-procfs" }

# Check if /proc exists and is a directory
ifelse -n { [ -d /proc ] }
{
  foreground { echo "/proc doesn't exist or isn't a directory! Aborting...\n\
(ERROR) Unable to start oneshot service:  mount-procfs"
  }
  exit 1
}
# Check if proc is already mounted
if -t -n { grep -wq proc /proc/mounts }
mount -v proc /proc -n -t proc
